{

  "manifest_version": 2,
  "name": "Wordie",
  "version": "1.0.0",

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [{
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["content_scripts.js"]  
  }],

  "permissions": [
    "*://www.dictionaryapi.com/api/v1/references/learners/xml/*"
  ]

}